#First put in your sources
execfile("/home/astro-util/projects/TKFPA/sources.cat")
# Set VEGAS configuration
execfile("/home/astro-util/projects/GAS/ds9_region_files/observing_scripts/vegas_kfpa_config")

target = "JUPITER"

Slew( target)
Balance()
#
#Print "Nodding beams 3 and 7"
Nod( target, "3", "7", scanDuration=30.0)
#Print "Nodding beams 2 and 6"
Nod( target, "2", "6", scanDuration=30.0)
#Print "Nodding beams 4 and 1"
Nod( target, "4", "1", scanDuration=30.0)
#Print "Nodding beams 1 and 5"
Nod( target, "1", "5", scanDuration=30.0)






